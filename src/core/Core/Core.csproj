<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/pfpack/pfpack-core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/pfpack/pfpack-core</RepositoryUrl>
    <Company>pfpack</Company>
    <Authors>Andrei Sergeev, Pavel Moskovoy</Authors>
    <Copyright>Copyright Â© 2020-2021 Andrei Sergeev, Pavel Moskovoy</Copyright>
    <Description>PrimeFuncPack Core is a functional programming pack for .NET consisting of the fundamentals: Optional and Result monads, Failure type, Tagged Union, Unit type, and functional interfaces (SAM interfaces), as well as the useful extensions. The Pack supports both asynchronous and synchronous programming models.</Description>
    <RootNamespace>System</RootNamespace>
    <AssemblyName>PrimeFuncPack.Core</AssemblyName>
    <Version>2.0.0-preview.1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\..\docs\core\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PrimeFuncPack.Core.Failure" Version="2.0.0-preview.1.0.0" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Abstractions" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Abstractions.Async" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Abstractions.AsyncValue" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Abstractions.Sync" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Extensions" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Extensions.Async" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Extensions.AsyncValue" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Func.Extensions.Sync" Version="2.0.0-preview.1.0.1" />
    <PackageReference Include="PrimeFuncPack.Core.Optional" Version="1.2.0-preview.2.0.0" />
    <PackageReference Include="PrimeFuncPack.Core.Result" Version="1.2.0-preview.1.0.0" />
    <PackageReference Include="PrimeFuncPack.Core.Taggeds" Version="1.2.0-preview.2.0.0" />
    <PackageReference Include="PrimeFuncPack.Core.TaggedUnion" Version="1.2.0-preview.1.0.0" />
    <PackageReference Include="PrimeFuncPack.Core.Unit" Version="2.0.0-preview.1.0.0" />
  </ItemGroup>

</Project>
